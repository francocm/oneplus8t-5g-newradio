  <!-- RULE #1 -->
  <!-- We wait if we don't have location or we are not ONLINE.
  -->
  <if precond="none">
    <any_of>
      <not> <have_location /> </not>
      <not> <phone_operating_mode> ONLINE </phone_operating_mode> </not>
    </any_of>
    <then>
      <stop />
    </then>
  </if>
  
  <!-- RULE #2 -->
  <!-- Continue setting the capabilities.
  -->
  <svc_mode> FULL </svc_mode>
  <rat_order> L W T G C H </rat_order> <!-- 20170608 liuweibin, open TDS for full rat -->
  <rat_capability base="none">
    <include> G W L T C H </include> <!-- 20170608 liuweibin, open TDS for full rat -->
  </rat_capability>

  <!-- RULE #3 -->
  <!-- Set capability based on location.
  -->
  <select>

  <!-- sxlte rule -->
    <case>
      <all_of>
        <location_mcc_in list="america_mccs" />
        <not><imsi_mcc_in list="test_sim_mccs" /></not>
        <not><imsi_plmn_in list="test_sim_plmns" /></not>
      </all_of>
      <actions>
        <rat_capability base="none"> <include> G W L </include> </rat_capability> <!-- [OPCR#000112][20180830][Sean]  set default rat to GWL -->
        <ue_mode> CSFB </ue_mode>
        <rat_order> L W G </rat_order>
        <freq_list list="default" />
            <if>
              <cond> <serving_plmn_in list = "america_att_plmns" /> </cond>
              <then>
                <ue_mode> CSFB </ue_mode>
                <rat_order> L W G </rat_order>
                <rf_bands list="america_att_bands" />
                <!-- [2019/12/02][chris][Update CA band config sequence] NA_AT&T --> 
